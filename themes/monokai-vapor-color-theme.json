{
	"name": "Monokia Dark Soda",
	"type": "dark",
	"$schema": "vscode://schemas/color-theme",
	"colors": {
		"editor.background": "#202020",
		"editor.foreground": "#f8f8f2",
		"editor.lineHighlightBackground": "#3d3d3d",
		"editor.selectionBackground": "#403d3d",
		"editorCursor.foreground": "#f8f8f0",
		"editorWhitespace.foreground": "#3b3a32",
		"input.background": "#1d1d1d",
	},
	//yellow #FFEE99
	//pink #FF80F4
	//red #F92672
	//cyan #66D9EF
	//lime #A6E22E
	"tokenColors": [
		{
			"scope": "meta.class",
			"settings": {
				"foreground": "#FFADF8",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "punctuation.section.class.begin.python",
			"settings": {
				"fontStyle": "",
				"foreground": "#f8f8f2"
			}
		},

		{
			"scope": "meta.function-call.generic",
			"settings": {
				"foreground": "#FFDEFD"
			}
		},
		{
			"scope": "support.function.magic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e0ffa1"
			}
		},
		{
			"scope": "variable.language.special.self, variable.parameter.function.language.special.self, support.variable.magic",
			"settings": {
				"foreground": "#dbf9ff"
			}
		},
		// {
		// 	"scope": "strong",
		// 	"settings": {
		// 		"fontStyle": "bold"
		// 	}
		// },
		{
			"scope": "comment",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#fff1ad"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#e0ffa1"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#e0ffa1"
			}
		},
		{
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#e0ffa1"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#FF9FA2"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#FF9FA2",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#B6F3FF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#FFC4FA",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#FFC4FA",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#FFC4FA",
				"fontStyle": ""
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#FFA764",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#B6F3FF",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#FFADF8",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#B6F3FF",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#B6F3FF",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.type, support.class",
			"settings": {
				"foreground": "#B6F3FF",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "support.other.namespace, entity.name.type.namespace",
			"settings": {
				"foreground": "#FFB2F9"
			}
		},
		{
			"scope": "support.other.namespace.use-as.php",
			"settings": {
				"foreground": "#B6F3FF"
			}
		},
		{
			"scope": "variable.language.namespace.php",
			"settings": {
				"foreground": "#D66990"
			}
		},
		{
			"scope": "punctuation.separator.inheritance.php",
			"settings": {
				"foreground": "#ffa764"
			}
		},
		{
			"scope": "support.function.misc.css, support.constant.property-value.css, support.constant.font-name.css",
			"settings": {
				"foreground": "#fff1ad"
			}
		},
		{
			"scope": "meta.tag.template.value.twig, meta.tag.template.block.twig",
			"settings": {
				"foreground": "#CD5AC5"
			}
		},
		{
			"scope": "keyword.control.twig",
			"settings": {
				"foreground": "#E05D8C"
			}
		},
		{
			"scope": "variable.other.twig",
			"settings": {
				"foreground": "#E5A5E0"
			}
		},
		{
			"scope": "variable.other.property.twig",
			"settings": {
				"foreground": "#FFE1FC"
			}
		},
		{
			"scope": "constant.language.twig",
			"settings": {
				"foreground": "#FFD2A6"
			}
		},
		{
			"scope": "constant.numeric.twig",
			"settings": {
				"foreground": "#FFD0FB"
			}
		},
		{
			"scope": "support.function.twig",
			"settings": {
				"foreground": "#90E7F7"
			}
		},
		{
			"scope": "meta.function-call.other.twig",
			"settings": {
				"foreground": "#FAB85A"
			}
		},
		{
			"scope": "meta.function-call.twig",
			"settings": {
				"foreground": "#FAB85A"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#F8F8F0",
				"background": "#ffa764",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F8F8F0",
				"background": "#FFADF8"
			}
		},
		{
			"scope": "text source",
			"settings": {
				"background": "#3D3D3D55"
			}
		},
		{
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"foreground": "#E0EDDD",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#D3201F",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#BF3904",
				"fontStyle": ""
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#60CF30"
			}
		},
		{
			"scope": "meta.diff, meta.diff.range",
			"settings": {
				"foreground": "#3BC0F0"
			}
		},
		{
			"scope": "markup.heading.markdown, markup.heading.1.markdown, markup.heading.2.markdown",
			"settings": {
				"foreground": "#CCFF67"
			}
		},
		{
			"scope": "markup.bold.markdown",
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic.markdown",
			"settings": {
				"foreground": "#F8F8F2",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "punctuation.definition.bold.markdown, punctuation.definition.italic.markdown, punctuation.definition.raw.markdown",
			"settings": {
				"foreground": "#D3201F"
			}
		},
		{
			"scope": "markup.list.unnumbered.markdown",
			"settings": {
				"foreground": "#FFADF8"
			}
		},
		{
			"scope": "markup.list.numbered.markdown",
			"settings": {
				"foreground": "#FFADF8"
			}
		},
		{
			"scope": "markup.raw.block.markdown, markup.raw.inline.markdown",
			"settings": {
				"foreground": "#fff1ad"
			}
		},
		{
			"scope": "markup.quote.markdown, punctuation.definition.blockquote.markdown",
			"settings": {
				"foreground": "#6C71C4"
			}
		},
		// {
		// 	"scope": "meta.separator.markdown",
		// 	"settings": {
		// 		"foreground": "#FFADF8"
		// 	}
		// },
		{
			"scope": "meta.image.inline.markdown, markup.underline.link.markdown",
			"settings": {
				"foreground": "#8C8C8C",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "string.other.link.title.markdown, string.other.link.description.markdown",
			"settings": {
				"foreground": "#b6f3ff"
			}
		},
		{
			"scope": "punctuation.definition.link.markdown",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "punctuation.definition.metadata.markdown",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "punctuation.definition.string.begin.markdown, punctuation.definition.string.end.markdown, punctuation.definition.constant.markdown",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"scope": "punctuation.definition.template-expression, punctuation.section.embedded",
			"settings": {
				"foreground": "#ffa764"
			}
		},
		{
			"scope": "meta.template.expression",
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		}
	]
}